{
  "cli": {
    "version": ">= 3.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "DUO_API_URL": "https://api.bunk-app.in",
        "DUO_CHAT_URL": "wss://chat.bunk-app.in",
        "DUO_IMAGES_URL": "https://user-images.bunk-app.in",
        "DUO_AUDIO_URL": "https://user-audio.bunk-app.in",
        "DUO_STATUS_URL": "https://status.bunk-app.in",
        "DUO_INVITE_URL": "https://bunk-app.in",
        "DUO_PARTNER_URL": "https://partner.bunk-app.in"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "credentialsSource": "remote"
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
